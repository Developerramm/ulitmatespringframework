<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean class="com.app.beans.Student" id="student">
    	<property name="name" value="Ram kumar"></property>
    	<property name="email" value="ram@gmail.com"></property>
    	<property name="username" value="ramman"></property>
    	<property name="course" ref="course"></property>
    </bean>
    
    <bean class="com.app.beans.Course" id="course"></bean>
  

</beans>